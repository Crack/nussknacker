{
  "env": {
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": [
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "i18next"
  ],
  "rules": {
    "i18next/no-literal-string": ["warn", {
      "ignore": [
        "-icon", "glyphicon"
      ],
      "ignoreCallee": [
        "t", "withTranslation", "useTranslation",
        "reportEvent", "headerStyles",
        "_.get",
        "describe", "it", "expect", "toBe", "toEqual"
      ],
      "ignoreAttribute": [
        "svgFile", "bounds"
      ]
    }]
  }
}
